    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- npx tsx db/migrate.ts",
    "db:reset": "dotenv -e .env.local -- npx tsx db/reset.ts",
    "db:squash": "rm -rf supabase/migrations/* && pnpm db:generate && pnpm db:setup",
